    "def make_autopct(values):\n",
    "\n",
    "    def my_autopct(pct):\n",
    "        total = sum(values)\n",
    "        \n",
    "        # additional call to int is for type conversion\n",
    "        # not duplicated rounding\n",
    "        val = int(round(pct*total/100.0))\n",
    "        return '{p:.1f}%  ({v:d})'.format(p=pct,v=val)\n",
    "\n",
    "    return my_autopct"
   ]
